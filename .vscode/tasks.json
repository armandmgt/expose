{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--bin",
				"exposed"
			],
			"env": {
				"RUST_LOG": "debug,thrussh=debug"
			},
			"problemMatcher": [
				"$rustc",
				"$rust-panic"
			],
			"label": "rust: cargo run",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}